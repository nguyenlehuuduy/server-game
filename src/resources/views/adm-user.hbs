<div class="lo-update-user" style="position: absolute;  position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);width: 50%; height: 100%; z-index: 999999; display: none;">

    <div class="card mb-0">
        <div class="card-body">
            <h5 class="card-title fw-semibold mb-4">Cập nhật thông tin người dùng</h5>
            <form>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Họ và tên của bé<a
                            style="color: red">*</a></label>
                    <input type="text" class="form-control up-name" id="" aria-describedby="">
                    <div id="emailHelp" class="form-text">
                    </div>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Tên đăng nhập<a style="color: red">*</a></label>
                    <input type="text" class="form-control up-username" id="" aria-describedby="">
                    <div id="emailHelp" class="form-text">
                    </div>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Mật khẩu</label>
                    <input type="text" class="form-control up-password" id="" aria-describedby="">
                    <div id="emailHelp" class="form-text">
                    </div>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Ngày sinh<a style="color: red">*</a></label>
                    <input type="date" class="form-control up-birth" id="" aria-describedby="">
                    <div id="emailHelp" class="form-text">
                    </div>
                </div>

                <div class="mb-3">
                    <label for="disabledSelect" class="form-label">Giới tính</label>
                    <select id="disabledSelect" class="form-select up-sex">
                        <option>Nam</option>
                        <option>nữ</option>
                    </select>
                </div>
                <p class="alert-err" style="color: red;"></p>
                <button type="button" class="btn btn-primary btn-update-user">Submit</button>
            </form>
        </div>
    </div>
</div>
